{
  "version": 2,
  "buildCommand": "./gradlew :composeApp:vercelBuild",
  "outputDirectory": "build/wasm/packages/AZ900-Quiz-composeApp/kotlin",
  "installCommand": "echo 'No install needed for Gradle project'",
  "devCommand": "./gradlew :composeApp:wasmJsBrowserDevelopmentRun",
  "framework": null,
  "headers": [
    {
      "source": "/(.*\\.wasm)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    },
    {
      "source": "/(.*\\.mjs)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
